{
  "name": "My Little Face When",
  "description": "Search the popular reaction image database!",
  "version": "0.1.1",
  "manifest_version": 3,

  "omnibox": {
    "keyword": "mlfw"
  },

  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon-128.png"
  },

  "permissions": [
    "tabs",
    "webNavigation",
    "http://mylittlefacewhen.com/"
  ],

  "background": {
    "persistent": false,
    "scripts": [
      "js/background.js"
    ]
  }
}